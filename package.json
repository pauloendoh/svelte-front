{
  "name": "svelte-front",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "npx tsc --noEmit && svelte-check",
    "test": "vitest",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "sync": "npx tsx ./src/preSync.ts && npx orval --config ./orval.config.ts && svelte-check",
    "sync:clean": "npx tsx ./src/cleanOrval.ts && npm run sync"
  },
  "type": "module",
  "devDependencies": {
    "@iconify/svelte": "^4.0.2",
    "@skeletonlabs/skeleton": "2.10.2",
    "@skeletonlabs/tw-plugin": "0.4.0",
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@tailwindcss/forms": "0.5.9",
    "@tailwindcss/typography": "0.5.15",
    "@types/eslint": "^9.6.0",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "22.7.2",
    "autoprefixer": "10.4.20",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.36.0",
    "globals": "^15.0.0",
    "openapi-zod-client": "^1.18.2",
    "postcss": "8.4.47",
    "prettier": "^3.3.3",
    "prettier-plugin-svelte": "^3.1.2",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "svelte": "^4.2.7",
    "svelte-check": "^4.0.4",
    "tailwindcss": "3.4.13",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.0.0",
    "vite": "^5.0.3",
    "vite-plugin-tailwind-purgecss": "0.3.3",
    "vitest": "^2.0.0"
  },
  "dependencies": {
    "@floating-ui/dom": "1.6.11",
    "@tanstack/svelte-query": "^5.59.0",
    "axios": "^1.7.7",
    "dotenv": "^16.4.5",
    "js-cookie": "^3.0.5",
    "neverthrow": "^8.0.0",
    "orval": "^7.1.1",
    "timeago.js": "^4.0.2",
    "zod": "^3.23.8",
    "zod-validation-error": "^3.4.0"
  }
}
